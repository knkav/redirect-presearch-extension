{
  "name": "Kolompilo Redirect",
  "description": "Kostaras made the best tool to redirect presearch",
  "manifest_version": 2,
  "version": "2.0",
  "icons": {
    "16": "kolompilo16.png",
    "48": "kolompilo48.png",
    "128": "kolompilo128.png"
